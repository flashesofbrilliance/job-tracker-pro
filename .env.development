# Development Environment Configuration
# Job Tracker Pro - Japanese Sushi Discovery Engine

# Development Settings
NODE_ENV=development
DEBUG=true
PORT=8080
HOST=127.0.0.1

# Version Control
AUTO_VERSION=true
VERSION_STRATEGY=semantic  # semantic, timestamp, git-hash
CACHE_BUSTING=auto

# Build Settings
BUILD_TARGET=discovery
MINIFY_JS=false
MINIFY_CSS=false
SOURCE_MAPS=true

# Feature Flags
ENABLE_3D_SCENE=true
ENABLE_JAPANESE_PATTERNS=true
ENABLE_BIOMETRIC_TRACKING=false
ENABLE_PERFORMANCE_MONITORING=true

# Asset Paths
ASSET_BASE_URL=./
CDN_URL=
STATIC_CACHE_MAX_AGE=86400

# Development Tools
HOT_RELOAD=true
LIVERELOAD_PORT=35729
BROWSER_SYNC=true

# Quality Assurance
ESLINT_ENABLED=true
PRETTIER_ENABLED=true
ACCESSIBILITY_CHECKS=true
PERFORMANCE_BUDGET=true

# Analytics & Monitoring
TRACK_ERRORS=development
LOG_LEVEL=debug
METRICS_ENDPOINT=

# Security
CSP_ENABLED=false
HTTPS_ONLY=false

# Japanese Localization
DEFAULT_LOCALE=en-US
JAPANESE_FALLBACK=true
RTL_SUPPORT=false