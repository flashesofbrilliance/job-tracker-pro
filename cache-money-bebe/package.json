{
  "name": "cache-money-bebe",
  "version": "1.0.0",
  "description": "ðŸ’° The ultimate caching orchestration library that makes your app fast and your cache game strong! Featuring revolving door cache strategy, payload cache strap, and intelligent cache integration for high-performance web applications. Because cache money ain't cache problems! ðŸš€",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "build:analyze": "rollup -c --environment ANALYZE:true",
    "clean": "rimraf dist coverage docs",
    "dev": "rollup -c --watch --environment NODE_ENV:development",
    "dev:server": "nodemon examples/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "cross-env CI=true jest --coverage --watchAll=false",
    "test:integration": "jest --testMatch='**/integration/**/*.test.js'",
    "test:unit": "jest --testMatch='**/unit/**/*.test.js'",
    "test:performance": "node scripts/performance-test.js",
    "lint": "eslint src/**/*.js --cache",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint:ci": "eslint src/**/*.js --max-warnings 0",
    "format": "prettier --write 'src/**/*.{js,json,md}'",
    "format:check": "prettier --check 'src/**/*.{js,json,md}'",
    "typecheck": "tsc --noEmit",
    "docs": "jsdoc src -r -d docs -c jsdoc.json",
    "docs:serve": "cd docs && python -m http.server 8080",
    "validate": "npm run lint:ci && npm run typecheck && npm run test:ci",
    "prepublishOnly": "npm run validate && npm run build:prod",
    "postpublish": "git tag v$npm_package_version && git push --tags",
    "size": "size-limit",
    "size:why": "npx why-is-node-running",
    "benchmark": "node benchmarks/run-benchmarks.js",
    "init:project": "node scripts/init-project.js",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "ci:install": "npm ci --prefer-offline --no-audit",
    "prepare": "husky install"
  },
  "keywords": [
    "cache",
    "caching",
    "performance",
    "web-performance",
    "cache-orchestration",
    "revolving-door-cache",
    "payload-cache",
    "first-paint-load",
    "cache-integration",
    "resource-optimization",
    "cdn-sync",
    "cache-strategy"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/cache-money-bebe.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cache-money-bebe/issues"
  },
  "homepage": "https://github.com/yourusername/cache-money-bebe#readme",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.10.6",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.2",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "size-limit": "^11.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {},
  "peerDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "50 KB"
    }
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}